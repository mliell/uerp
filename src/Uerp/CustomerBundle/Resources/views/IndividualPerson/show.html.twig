{% extends '::base.html.twig' %}

{% block body -%}
    <h1>IndividualPerson</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Cpf</th>
                <td>{{ entity.cpf }}</td>
            </tr>
            <tr>
                <th>Rgnumber</th>
                <td>{{ entity.rgNumber }}</td>
            </tr>
            <tr>
                <th>Rgexpeditiondate</th>
                <td>{{ entity.rgExpeditionDate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Rgexpeditionlocal</th>
                <td>{{ entity.rgExpeditionLocal }}</td>
            </tr>
            <tr>
                <th>Occupation</th>
                <td>{{ entity.occupation }}</td>
            </tr>
            <tr>
                <th>Workplace</th>
                <td>{{ entity.workplace }}</td>
            </tr>
            <tr>
                <th>Salary</th>
                <td>{{ entity.salary }}</td>
            </tr>
            <tr>
                <th>Spousename</th>
                <td>{{ entity.spouseName }}</td>
            </tr>
            <tr>
                <th>Fathername</th>
                <td>{{ entity.fatherName }}</td>
            </tr>
            <tr>
                <th>Mothername</th>
                <td>{{ entity.motherName }}</td>
            </tr>
            <tr>
                <th>Gender</th>
                <td>{{ entity.gender }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.name }}</td>
            </tr>
            <tr>
                <th>Phone_number</th>
                <td>{{ entity.phonenumber }}</td>
            </tr>
            <tr>
                <th>Mobile_number</th>
                <td>{{ entity.mobilenumber }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.email }}</td>
            </tr>
            <tr>
                <th>Birth_date</th>
                <td>{{ entity.birthdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ entity.street }}</td>
            </tr>
            <tr>
                <th>Street_number</th>
                <td>{{ entity.streetnumber }}</td>
            </tr>
            <tr>
                <th>Complement</th>
                <td>{{ entity.complement }}</td>
            </tr>
            <tr>
                <th>District</th>
                <td>{{ entity.district }}</td>
            </tr>
            <tr>
                <th>Postal_code</th>
                <td>{{ entity.postalcode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ entity.city }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ entity.notes }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('individualperson') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('individualperson_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
